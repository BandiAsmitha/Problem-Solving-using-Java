import java.util.*;
class swaptwonums
{
    public static void main(String args[])
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter two numbers:");
        int a= sc.nextInt();
        int b=sc.nextInt();
        a=a+b;
        b=a-b;
        a=a-b;
        System.out.println("After swapping:");
        System.out.printf("a=%d and b=%d",a,b);
    }
}
